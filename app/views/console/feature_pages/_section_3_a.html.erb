<div section_id="section_3_a" class="section">
	<%= render '/console/feature_pages/section_actions' %>
	<ul col_width="310" class="inner3ColsEqual sortable">
		<% left_element_ids.each do |element_id| %>
			<%= render :partial => 'console/elements/element_item', :locals => {:element_id => element_id, :elements => elements} %>
		<%end%>
		<%= render "console/elements/add_element_action" %>
	</ul>
	<ul col_width="310" class="inner3ColsEqual sortable">
		<% center_element_ids.each do |element_id| %>
			<%= render :partial => 'console/elements/element_item', :locals => {:element_id => element_id, :elements => elements} %>
		<%end%>
		<%= render "console/elements/add_element_action" %>
	</ul>
	<ul col_width="310" style="margin:0" class="inner3ColsEqual sortable">
		<% right_element_ids.each do |element_id| %>
			<%= render :partial => 'console/elements/element_item', :locals => {:element_id => element_id, :elements => elements} %>
		<%end%>
		<%= render "console/elements/add_element_action" %>
	</ul>
	<div class="clear"></div>
</div>

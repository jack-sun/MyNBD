<ul class="userWall">
	<% users.each do |user| %>
	<li>
		<%= link_to image_tag(user_avatar_path(user, :middle), {:name => "userhead", :width => "50", :height => "50"}), user_url(user), :class => "avatar" %>
		<div class="name"><%= link_to user.nickname, user_url(user) %></div>
	</li>
	<%end %>
</ul>
<div class="clear"></div>

          <% end %>
        </div>
        <div>
          <% cache(column_show_content_key_by_id(@delta_zhejiang_articles[:id])) do %>
          <%= render :partial => "article_list_v3", :locals => { :digest_number => 1, :articles => articles_of(@delta_zhejiang_articles), :img_ver => "thumb_hm", :mask_size => "120x80" } %>
          <% end %>
        </div>
        <div>
          <% cache(column_show_content_key_by_id(@delta_jiangsu_articles[:id])) do %>
          <%= render :partial => "article_list_v3", :locals => { :digest_number => 1, :articles => articles_of(@delta_jiangsu_articles), :img_ver => "thumb_hm", :mask_size => "120x80" } %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript">
  $.nbdslide.init({
    optDOMs:[{
      selector:"#sh_csj_slide",
      style:"normal"
    }],
    indexDOM:"#sh_csj_index"
  });
</script>
        <% end %>
      </div>

      <div class="main">
        <% cache(column_show_content_key_by_id(@head_article[:id])) do %>
        <%= render :partial => 'column_breakingnews', :layout => 'story', :locals => { :articles => articles_of(@head_article) } %>
        <% end %>
      </div>
    </div>

    <div class="story_shadow">
      <div class="story">
        <div class="story_divide_l">
          <% cache(column_show_content_key_by_id(@hot[:id])) do %>
            <%= render :partial => "article_list_v3", :layout => "story", :locals => { :story_is_hide => true, :header_data => {:list_title => "热公司"}, :column_id => column_id_of(@hot), :digest_number => 2, :articles => articles_of(@hot) } %>
          <% end %>
        </div>
        <div class="story_divide_r">
          <% cache(column_show_content_key_by_id(@sicence[:id])) do %>

          <% end %>
        </div>
      </div>
    </div>

    <div class="story_shadow">
      <div class="story">
        <div class="story_divide_l">
          <% cache(column_show_content_key_by_id(@auto[:id])) do %>
            <%= render :partial => "article_list_v3", :layout => "story", :locals => { :story_is_hide => true, :header_data => {:list_title => "汽车"}, :column_id => column_id_of(@auto), :digest_number => 2, :articles => articles_of(@auto) } %>

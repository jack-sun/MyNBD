<% @title="公司频道" %>
<%= require_slideShow %>

<% content_for :header_ad do %>
  <script type="text/javascript">
    BAIDU_CLB_preloadSlots("347545","347546","349119","349120");
  </script>
  <!-- 广告位：公司_顶部四分之三通栏 -->
  <%= render :partial => "ads/ad", :locals => { :id => "347545", :html_class => "header" } %>
<% end %>

<% cache(column_sub_nav_key_by_id(33)) do%>
<%= render :partial => "sub_nav", :locals => {:column_id => 33 } %>
<% end %>

<div class="layout">

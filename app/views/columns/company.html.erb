          <% end %>
        </div>
      </div>
    </div>
    
    <!-- 广告位：公司_左通栏一 -->
    <%= render :partial => "ads/ad", :locals => { :id => "347546", :html_class => "left_bar" } %>

    <div class="story_shadow">
      <div class="story">
        <div class="story_divide_l">
          <% cache(column_show_content_key_by_id(@property[:id])) do %>
            <%= render :partial => "article_list_v3", :layout => "story", :locals => { :story_is_hide => true, :header_data => {:list_title => "房产"}, :column_id => column_id_of(@property), :digest_number => 2, :articles => articles_of(@property) } %>
